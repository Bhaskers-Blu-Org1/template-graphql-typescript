{
  "name": "template-node-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "protocol": "http",
    "host": "localhost",
    "port": 3000,
    "contextRoot": ""
  },
  "jest": {
    "setupFiles": [
      "jest-plugin-context/setup"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "logs/.*",
      "dist/.*",
      "pacts/.*",
      "coverage/",
      "src/logger/.*"
    ],
    "coverageDirectory": "coverage",
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "pact": {
    "providerBaseUrl": "",
    "pactBrokerUrl": "",
    "provider": "",
    "pactUrls": [
      "pacts/hello-consumer-typescript-template.json"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "pretest": "npm run build",
    "test": "jest --coverage",
    "tdd": "jest --watch",
    "prebuild": "npm run clean",
    "build": "tsc",
    "build:schema": "ts-node src/schema.ts",
    "prestart": "npm run build",
    "start": "npm run serve",
    "serve": "node dist/src",
    "upgrade-interactive": "npm-check --update",
    "pact:verify": "ts-node test/pact-verify",
    "pact:publish": "pact publish -p ./pacts -c $npm_package_version -b $npm_package_pact_pactBrokerUrl",
    "generate-exports": "echo \"export IMAGE_NAME=\\\"$npm_package_name\\\";\nexport IMAGE_VERSION=\\\"$npm_package_version\\\";\nexport IMAGE_BUILD_NUMBER=\\\"$BUILD_NUMBER\\\";\nexport START_TIME=\\\"$(date -u +%Y-%m-%dT%H:%M:%SZ)\\\";\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-prettystream": "^0.1.3",
    "cors": "^2.8.5",
    "graphiql": "^0.13.2",
    "graphql": "^14.4.2",
    "graphql-yoga": "^1.18.1",
    "reflect-metadata": "^0.1.13",
    "superagent": "^5.0.5",
    "tslib": "^1.10.0",
    "type-graphql": "^0.17.4",
    "typescript-ioc": "^1.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@pact-foundation/pact": "^8.2.2",
    "@pact-foundation/pact-node": "^8.2.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.11",
    "@types/jest-plugin-context": "^2.9.2",
    "@types/node": "^11.13.6",
    "@types/superagent": "^4.1.1",
    "@types/yargs": "^13.0.0",
    "babel-jest": "^24.7.1",
    "easygraphql-tester": "^5.1.6",
    "jest": "^24.7.1",
    "jest-plugin-context": "^2.9.0",
    "npm-check": "^5.9.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4",
    "yargs": "^13.2.2"
  }
}
